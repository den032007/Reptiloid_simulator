[project]
title = Reptiloid Simulator
version = 0.1
dependencies = https://github.com/Jrayp/Moku/archive/Alpha_v2.2.zip,https://github.com/britzl/monarch/archive/master.zip

[bootstrap]
main_collection = /loader.collectionc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 1280
height = 720

[physics]
scale = 0.02
max_collisions = 256
max_contacts = 512
trigger_overlap_capacity = 64

[script]
shared_state = 1

